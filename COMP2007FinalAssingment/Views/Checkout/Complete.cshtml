@model int
@{
    ViewBag.Title = "Checkout Complete";
}
<div id="content">
    <div class="container">

        <div class="col-md-12">
            <ul class="breadcrumb">
                <li>
                    <a href="@Url.Action("Index","Store")">Store</a>
                </li>
                <li>Checkout Complete</li>
            </ul>
        </div>

        <div class="col-md-12" id="checkout">

            <div class="box">
                <form method="post" action="checkout4.html">
                    <h1>Checkout - Order review</h1>
                    <ul class="nav nav-pills nav-justified">
                        <li class="active">
                            <a href="#"><i class="fa fa-eye"></i><br>Order Review</a>
                        </li>
                    </ul>

                    <div class="content">
                     
                        <h4>Thanks for your order! Your order number is: @Model</h4>
                    </div>
                    <!-- /.content -->

                    <div class="box-footer">
                        <div class="pull-left">
                            <a href="@Url.Action("Index","Store")" class="btn btn-default"><i class="fa fa-chevron-left"></i>Back to Store</a>
                        </div>
                       
                    </div>
                </form>
            </div>
            <!-- /.box -->


        </div>
        <!-- /.col-md-9 -->

        <div class="col-md-3">

            

        </div>
        <!-- /.col-md-3 -->

    </div>
    <!-- /.container -->
</div>
<!-- /#content -->
